<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=System.Runtime">

    <System:String x:Key="HelloWorldLabel">Hello</System:String>
    <!-- Home Layout -->
    <System:String x:Key="HomeLayoutLabel1">No saved calculations yet</System:String>
    <System:String x:Key="HomeLInterestLabel1">Make a new calculation and save it</System:String>
    <!-- Billing Layout -->
    <System:String x:Key="BillingLayoutLabel1">Billing</System:String>
    <System:String x:Key="BillingInterestLabel1">Interest Amount:</System:String>
    <System:String x:Key="BillingIntererstInRBox1">Monthly</System:String>
    <System:String x:Key="BillingIntererstInRBox2">Yearly</System:String>
    <System:String x:Key="BillingInterestTextBox1">Enter monthly/yearly interest rate... e.g., 12</System:String>
    <System:String x:Key="BillingMonthlyInLabel">Monthly Incomes:</System:String>
    <System:String x:Key="BillingMonthlyInTextBox1" xml:space="preserve">Enter monthly incomes... One line per month: e.g.
1=&gt;30000
2=&gt;5000
3=&gt;0</System:String>
    <System:String x:Key="BillingInterestOutLabel1">Interest Income:</System:String>
    <System:String x:Key="BillingInterestOutRBox1">Transfer In</System:String>
    <System:String x:Key="BillingInterestOutRBox2">Transfer Out</System:String>
